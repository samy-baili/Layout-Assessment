<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SquareCardView">
        <attr name="iconCard" format="integer" />
        <attr name="labelCard" format="string" />
        <attr name="isSelected" format="boolean" />
    </declare-styleable>


    <declare-styleable name="AddArticleView">
        <attr name="brand" format="string" />
